{
    "enabled": 1,
    "hidden": false,
    "description": "jobsets",
    "nixexprinput": "nix-packages",
    "nixexprpath": "hydra/jobsets.nix",
    "checkinterval": 3600,
    "schedulingshares": 100,
    "enableemail": false,
    "emailoverride": "",
    "keepnr": 1,
    "inputs": {
        "nix-packages": {
            "type": "git",
            "value": "https://git.chir.rs/dark-kirb/nix-packages.git main",
            "emailresponsible": false
        },
        "nixpkgs": {
            "type": "git",
            "value": "https://github.com/NixOS/nixpkgs.git",
            "emailresponsible": false
        },
        "prsJSON": {
            "type": "path",
            "value": "https://git.chir.rs/api/v1/repos/darkkirb/nix-packages/pulls?state=open&sort=recentupdate",
            "emailresponsible": false
        },
        "gitea_status_repo": {
            "type": "string",
            "value": "nix-packages",
            "emailresponsible": false
        },
        "gitea_http_url": {
            "type": "string",
            "value": "https://git.chir.rs/",
            "emailresponsible": false
        },
        "gitea_repo_owner": {
            "type": "string",
            "value": "darkkirb",
            "emailresponsible": false
        },
        "gitea_repo_name": {
            "type": "string",
            "value": "nix-packages",
            "emailresponsible": false
        }
    }
}
